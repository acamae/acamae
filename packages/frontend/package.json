{
  "name": "@gestion-esports/frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "axios": "^1.8.4",
    "bootstrap": "^5.3.3",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.1",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^15.4.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^7.5.0",
    "redux": "^5.0.1",
    "terser-webpack-plugin": "^5.3.14",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/axios": "^0.9.36",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-redux": "^7.1.30",
    "@types/react-router-dom": "^5.3.3",
    "@types/zxcvbn": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "commitizen": "^4.3.1",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.10.0",
    "cypress": "^14.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^14.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^15.5.1",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "serve": "^14.2.4",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.3",
    "web-vitals": "^4.2.4",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.1"
  },
  "scripts": {
    "start": "npx webpack-dev-server --progress --config webpack.config.js --mode development --host 0.0.0.0 --port 3000 --env development",
    "build": "webpack --config webpack.config.js --mode production --env production",
    "build:analyze": "webpack --config webpack.config.js --mode production --env production --env analyze",
    "preview": "npm run build && echo 'El frontend est√° listo en /dist para ser servido por NGINX.'",
    "test": "jest --coverage",
    "check:coverage": "node checkCoverage.mjs",
    "test:coverage": "npm run test && npm run check:coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:prod": "NODE_ENV=production cypress run",
    "test:e2e:prod:open": "NODE_ENV=production cypress open",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,scss}\"",
    "prepare": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
